# conf shared between the jenkins master and agent roles
jenkins::slave::version: '3.14'
# Note  that jenkins very strangely seems to require the `repo` (full) scope in
# order to process a github personal api token.
jenkins::slave::ui_user: ENC[PKCS7,]
jenkins::slave::ui_pass: ENC[PKCS7,]
# pipeline scripts will try to restart on a agent based its name; if an agent
# with an unique id in its name is restarted, it reattaches with a new unique
# name and the pipeline script hangs forever.
jenkins::slave::disable_clients_unique_id: true
jenkins::slave::delete_existing_clients: true
docker::version: '18.09.1'
timezone::timezone: 'US/Pacific'
tuned::profile: 'virtual-host'
