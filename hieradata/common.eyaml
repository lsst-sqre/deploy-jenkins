---
jenkinsx:
  ssh_private_key: &jenkinsx_ssh_private_key |
    ENC[PKCS7,]
jenkinsx::authorization_strategy:
  org.jenkinsci.plugins.GithubAuthorizationStrategy:
      arguments:
        - 'jhoblitt, frossie'
        - false
        - false
        - false
        - 'lsst, lsst-sqre, lsst-dm'
        - false
        - false
        - false
        - true
jenkinsx::security_realm:
  org.jenkinsci.plugins.GithubSecurityRealm:
    arguments:
      - https://github.com
      - https://api.github.com
      - ENC[PKCS7,]
      - ENC[PKCS7,]
      - read:org
jenkinsx::credentials:
  jenkins-admin-ssh-private-key:
    domain: null
    scope: SYSTEM
    impl: BasicSSHUserPrivateKey
    description: 'jenkins admin'
    username: ENC[PKCS7,]
    private_key: ENC[PKCS7,]
  github-api-token-jhoblitt:
    domain: null
    scope: SYSTEM
    impl: StringCredentialsImpl
    description: 'github API personal access token (jhoblitt)'
    secret: ENC[PKCS7,]
jenkins::slave::ui_user: ENC[PKCS7,]
jenkins::slave::ui_pass: ENC[PKCS7,]
jenkins::slave::version: '2.0'
jenkins::plugins::hipchat:
    token: ENC[PKCS7,]
    room: 'Test Bot: Xenkins'
    sendas: Jenkins
jenkins::jobs::jenkins_ebs_snapshot:
  aws_access_key_id: ENC[PKCS7,]
  aws_secret_access_key: ENC[PKCS7,]
jenkinsx::nodes::lsst_dev: false
jenkinsx::user:
  jhoblitt:
    api_token_plain: ENC[PKCS7,]
    public_keys:
      - 'ssh-rsa  jenkins-demo'
jenkins::swarm::version: '1.22'
lsststack::lsstsw::lsstsw_repo: 'https://github.com/lsst/lsstsw.git'
lsststack::lsstsw::lsstsw_branch: master
lsststack::lsstsw::lsstsw_ensure: latest
lsststack::lsstsw::buildbot_repo: 'https://github.com/lsst-sqre/buildbot-scripts.git'
lsststack::lsstsw::buildbot_branch: master
lsststack::lsstsw::buildbot_ensure: latest
lsststack::lsstsw::lsst_build_repo: 'https://github.com/lsst/lsst_build.git'
lsststack::lsstsw::lsst_build_branch: master
lsststack::lsstsw::lsst_build_ensure: present
nginx::config::confd_purge: true
nginx::config::vhost_purge: true
nginx::config::http2: 'on'
# https://mozilla.github.io/server-side-tls/ssl-config-generator/?server=nginx-1.7.12&openssl=1.0.1e&hsts=yes&profile=modern
# enabled TLSv1 to support android < 4.4
