---
# yamllint disable rule:line-length
lookup_options:
  classes:
  jenkins::plugin_hash:
    merge:
      strategy: hash
  jenkinsx::casc:
    merge:
      strategy: deep
      merge_hash_arrays: false
      sort_merged_arrays: false
  files:
    merge:
      strategy: deep
jenkins::plugin_hash:
  ant:
    version: '1.9'
  mapdb-api:
    version: '1.0.9.0'
  resource-disposer:
    version: '0.12'
  subversion:
    version: '2.12.1'
  ws-cleanup:
    version: '0.37'
  robot:
    version: '1.6.5'
jenkinsx::casc:
  jenkins:
  unclassified:
    githubpluginconfig:
      configs:
        - credentialsId: "github-pat-lssttsadmin-webhooks"
    mailer:
      # casc is presently unable to configure smtp auth
      smtpHost: email-smtp.us-west-2.amazonaws.com
      useSsl: true
      smtpPort: 465
      charset: UTF-8
  jobs:
    - script: |
        folder('sqre') {
          description('SQRE mission related jobs')
        }
        folder('sqre/seeds') {}

        job('sqre/seeds/tssw-jobs') {
          //label('jenkins-master')
          concurrentBuild(false)

          scm {
            git {
              remote {
                url('https://github.com/lsst-sqre/jenkins-tssw-jobs')
              }
              branch('*/master')
            }
          } // scm

          triggers {
            githubPush()
          }

          steps {
            jobDsl {
              targets('jobs/*.groovy')
              useScriptText(false)
              ignoreExisting(false)
              ignoreMissingFiles(false)
              removedJobAction('DELETE')
              removedViewAction('IGNORE')
              lookupStrategy('JENKINS_ROOT')
            }
          } // steps
        } // job
#  credentials:
