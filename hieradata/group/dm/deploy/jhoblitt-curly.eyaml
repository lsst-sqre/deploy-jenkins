---
# yamllint disable rule:line-length
lookup_options:
  jenkinsx::casc:
    merge:
      strategy: deep
      merge_hash_arrays: false
      sort_merged_arrays: false
jenkinsx::versiondb:
  ssh_private_key: &jenkinsx_versiondb_ssh_private_key |
    ENC[PKCS7,]
  ssh_public_key: |
    ssh-rsa  jhoblitt-curly@versiondb
jenkinsx::casc:
  jenkins:
    securityRealm:
      github:
        clientID: "${github_oauth_client_id}"
        clientSecret: "${github_oauth_client_secret}"
        githubApiUri: https://api.github.com
        githubWebUri: https://github.com
        oauthScopes: read:org
  credentials:
    system:
      domainCredentials:
        - credentials:
            - string:
                id: slack-default-channel
                scope: GLOBAL
                description: Default slack channel
                secret: dmj-jhoblitt-curly
            - string:
                id: slack-channel-prefix
                scope: GLOBAL
                description: Prefix for generated slack channels
                secret: dmjjc
            - string:
                id: github-api-token-sqreadmin
                scope: GLOBAL
                description: github API personal access token (sqreadmincurly)
                secret: ENC[PKCS7,]
            - basicSSHUserPrivateKey:
                id: github-jenkins-versiondb
                scope: GLOBAL
                description: github provingground-curly/versiondb deploy key
                username: git
                privateKeySource:
                  directEntry:
                    privateKey: *jenkinsx_versiondb_ssh_private_key
            - usernamePassword:
                id: dockerhub-sqreadmin
                scope: GLOBAL
                description: dockerhub (sqreadmincurly)
                username: ENC[PKCS7,]
                password: ENC[PKCS7,]
files:
  /etc/jenkins/vault:
    content: |
      CASC_VAULT_TOKEN=ENC[PKCS7,]
      CASC_VAULT_PATHS=secret/dm/square/jenkins/jhoblitt-curly/scipipe-publish,secret/dm/square/jenkins/jhoblitt-curly/slack,secret/dm/square/jenkins/jhoblitt-curly/ghslacker,secret/dm/square/jenkins/jhoblitt-curly/github_oauth
      CASC_VAULT_URL=https://vault.lsst.codes
      CASC_VAULT_ENGINE_VERSION=2
    owner: jenkins
    group: jenkins
    mode: '0640'
