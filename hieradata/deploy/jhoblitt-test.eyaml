---
lookup_options:
  jenkinsx::credentials:
    merge: hash
jenkinsx::security_realm:
  org.jenkinsci.plugins.GithubSecurityRealm:
    arguments:
      - https://github.com
      - https://api.github.com
      - ENC[PKCS7,]
      - ENC[PKCS7,]
      - read:org
jenkinsx::credentials:
  slack-default-channel:
    domain:
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: Default slack channel
    secret: &slack_default_channel 'dm-jenkins-test'
  slack-channel-prefix:
    domain:
    scope: GLOBAL
    impl: StringCredentialsImpl
    description: Prefix for generated slack channels
    secret: 'dmjt'
jenkins::plugins::slack:
  teamdomain: lsstc
  tokencredentialid: slack-lsstc-token
  room: *slack_default_channel
jenkinsx::nodes:
  elcapitan-4:
    template: "%{module_name}/nodes/osx/config.xml.epp"
    params:
      name: elcap-4
      host: jenkins-elcap-4.lsst.cloud
      label: osx osx-10.11 elcap-3
